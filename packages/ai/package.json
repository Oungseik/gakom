  },
  "devDependencies": {
    "@repo/config": "workspace:*",
    "@types/bun": "^1.3.8",
    "dotenv-cli": "^11.0.0",
    "tsdown": "0.20.3",
    "typescript": "^5.9.3"
  }
}
