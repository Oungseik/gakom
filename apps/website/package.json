{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "website",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "bunx --bun vite dev",
    "build": "bunx --bun vite build",
    "preview": "bunx --bun vite preview --host",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check-types": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint .",
    "test:unit": "bunx --bun vitest",
    "test": "bun run test:unit -- --run"
  },
  "devDependencies": {
    "@eslint/compat": "^2.0.0",
    "@eslint/js": "^9.39.2",
    "@sveltejs/adapter-auto": "^7.0.0",
    "@sveltejs/kit": "^2.49.2",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/svelte": "^5.3.1",
    "@trivago/prettier-plugin-sort-imports": "^6.0.0",
    "@types/bun": "^1.3.5",
    "@types/node": "^24.10.4",
    "@types/nodemailer": "^7.0.4",
    "bits-ui": "^2.14.4",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-svelte": "^3.13.1",
    "globals": "^16.5.0",
    "jsdom": "^27.4.0",
    "mdsvex": "^0.12.6",
    "prettier": "^3.7.4",
    "prettier-plugin-svelte": "^3.4.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "svelte": "^5.46.1",
    "svelte-adapter-bun": "^1.0.1",
    "svelte-check": "^4.3.5",
    "svelte-meta-tags": "^4.5.0",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.1",
    "vite": "npm:rolldown-vite@^7.3.0",
    "vitest": "^4.0.16"
  },
  "dependencies": {
    "@fontsource/inter": "^5.2.8",
    "@inlang/paraglide-js": "^2.7.1",
    "@lucide/svelte": "^0.555.0",
    "@orpc/client": "^1.13.1",
    "@orpc/server": "^1.13.1",
    "@orpc/tanstack-query": "^1.13.1",
    "@repo/db": "workspace:*",
    "@repo/ui": "workspace:*",
    "@tanstack/svelte-form": "^1.27.6",
    "@tanstack/svelte-query": "^6.0.10",
    "@tanstack/table-core": "^8.21.3",
    "better-auth": "^1.4.9",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "libphonenumber-js": "^1.12.33",
    "mode-watcher": "^1.1.0",
    "nodemailer": "^7.0.12",
    "rate-limiter-flexible": "^9.0.1",
    "runed": "^0.37.1",
    "sharp": "^0.34.5",
    "svelte-sonner": "^1.0.7",
    "zod": "^4.2.1"
  }
}
