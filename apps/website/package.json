{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "website",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "bunx --bun vite dev",
    "build": "bunx --bun vite build",
    "preview": "bunx --bun vite preview --host",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check-types": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint .",
    "test:unit": "bunx --bun vitest",
    "test": "npm run test:unit -- --run"
  },
  "devDependencies": {
    "@eslint/compat": "^2.0.0",
    "@eslint/js": "^9.39.1",
    "@sveltejs/adapter-auto": "^7.0.0",
    "@sveltejs/kit": "^2.49.0",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.1.17",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/svelte": "^5.2.9",
    "@trivago/prettier-plugin-sort-imports": "^6.0.0",
    "@types/node": "^24.10.1",
    "@types/nodemailer": "^7.0.4",
    "bits-ui": "^2.14.4",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-svelte": "^3.13.0",
    "globals": "^16.5.0",
    "jsdom": "^27.2.0",
    "mdsvex": "^0.12.6",
    "prettier": "^3.7.3",
    "prettier-plugin-svelte": "^3.4.0",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "svelte": "^5.45.2",
    "svelte-check": "^4.3.4",
    "svelte-meta-tags": "^4.5.0",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.0",
    "vite": "npm:rolldown-vite@^7.2.8",
    "vitest": "^4.0.14"
  },
  "dependencies": {
    "@fontsource/inter": "^5.2.8",
    "@inlang/paraglide-js": "^2.5.0",
    "@lucide/svelte": "^0.555.0",
    "@orpc/client": "^1.12.0",
    "@orpc/server": "^1.12.0",
    "@orpc/tanstack-query": "^1.12.0",
    "@repo/db": "workspace:*",
    "@repo/ui": "workspace:*",
    "@tanstack/svelte-query": "^6.0.9",
    "better-auth": "^1.4.4",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "libphonenumber-js": "^1.12.30",
    "mode-watcher": "^1.1.0",
    "nodemailer": "^7.0.11",
    "rate-limiter-flexible": "^9.0.0",
    "runed": "^0.37.0",
    "svelte-sonner": "^1.0.6",
    "zod": "^4.1.13"
  }
}
